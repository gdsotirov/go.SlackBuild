# Fix for tzdata newer than 2016g
# See https://github.com/golang/go/issues/17276
# See https://github.com/golang/go/commit/c5434f2973a87acff76bac359236e690d632ce95
diff -urNad go1.7.4-orig/src/time/time_test.go go1.7.4/src/time/time_test.go
--- go1.7.4-orig/src/time/time_test.go	2016-12-01 23:16:28.000000000 +0200
+++ go1.7.4/src/time/time_test.go	2016-12-02 09:46:09.631968111 +0200
@@ -939,8 +939,11 @@
 	// but Go and most other systems use "east is positive".
 	// So GMT+1 corresponds to -3600 in the Go zone, not +3600.
 	name, offset := Now().In(loc).Zone()
-	if name != "GMT+1" || offset != -1*60*60 {
-		t.Errorf("Now().In(loc).Zone() = %q, %d, want %q, %d", name, offset, "GMT+1", -1*60*60)
+	// The zone abbreviation is "-01" since tzdata-2016g, and "GMT+1"
+	// on earlier versions; we accept both. (Issue #17276).
+	if !(name == "GMT+1" || name == "-01") || offset != -1*60*60 {
+		t.Errorf("Now().In(loc).Zone() = %q, %d, want %q or %q, %d",
+			name, offset, "GMT+1", "-01", -1*60*60)
 	}
 }
 
